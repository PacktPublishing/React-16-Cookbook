{
  "name": "webpack-node-react",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "clean": "rm -rf public/app",
    "start": "npm run clean && NODE_ENV=development nodemon src/server --watch src/server --exec babel-node --presets es2015",
    "start-production": "npm run clean && npm run build && NODE_ENV=production babel-node src/server --presets es2015"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "express": "~4.15.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "morgan": "~1.9.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.6.0"
  }
}
